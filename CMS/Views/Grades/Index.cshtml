@model IEnumerable<CMS.Models.Grade>

@{
    ViewBag.Title = "Index";
}

<h2>List of Grades</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Grade_Des)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Grade_Status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Grade_Des)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Grade_Status)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Grade_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Grade_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Grade_Id })
        </td>
    </tr>
}

</table>
